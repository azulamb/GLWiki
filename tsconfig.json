{
	"compilerOptions": {
		"outFile": "./docs/app.js",
		"module": "system",
		"moduleResolution": "node",
		"target": "es5",
		"lib": [
			"dom",
			"es2017"
		],
		"strictNullChecks": true,
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": false,
		"sourceMap": false
	},
	"include": [
		"./src/*.ts"
	]
}